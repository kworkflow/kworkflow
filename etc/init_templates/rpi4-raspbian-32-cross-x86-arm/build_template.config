# Specify the default architecture used by KW
arch=arm

# Use this flag as a way to indicate to kw the kernel image name. If this
# option is not specified, kw will try to infer the correct file inside
# arch/*/boot/.
kernel_img_name=kernel7l

# Specify cross-compile name
# 32bits
cross_compile=arm-linux-gnueabihf-

# Default kernel menu config option
menu_config=nconfig

# Generate kernel-doc, user can use any option currently available in the
# Kernel (e.g., pdfdocs, latexdocs, cleandocs, etc.)
doc_type=htmldocs

# Specify what percentage of the CPU should be utilized when building
cpu_scaling_factor=100

# Enable compilation cache
#enable_ccache=true

# This option can be used to enable compilation warnings accordingly.
# warning_level=2

# This option will save the full compilation log
# log_path=./output.log

# Use the LLVM toolchain
#use_llvm=yes

# Specify compilation flags
#cflags=